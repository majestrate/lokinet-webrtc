{
  "name": "lokinet-webrtc",
  "version": "0.0.1",
  "description": "lokinet webrtc shim jizz",
  "main": "index.js",
  "scripts": {
    "demo": "electron .",
    "dist": "electron-webpack ; rm -rf dist; rm -rf release; electron-builder",
    "win32": "electron-webpack ; rm -rf dist; rm -rf release; electron-builder --windows portable"
  },
  "author": "jeff",
  "license": "GPL-3.0",
  "devDependencies": {
    "electron": "^13.1.7",
    "electron-builder": "^22.11.7",
    "ws": "^8.0.0"
  },
  "electronWebpack": {
    "title": true,
    "whiteListedModules": ["ws"],

    "main": {
      "sourceDirectory": "."
    },
    "renderer": {
      "sourceDirectory": "."
    }
  },

  "build": {
    "appId": "com.electron.lokinet.webrtc",
    "directories": {
      "output": "release/"
    },
    "asar": true,
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {}
  },
  "dependencies": {
    "electron-webpack": "^2.8.2"
  }
}
